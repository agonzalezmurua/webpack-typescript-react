{
    "presets": [
        "@babel/preset-react",
        "@babel/preset-typescript",
        [
            "@babel/preset-env", {
                "useBuiltIns": "usage",
                "corejs": { "version": 3, "proposals": true },
                "modules": false
            }
        ]
    ],
    "plugins": [
        "react-hot-loader/babel",
        "@babel/plugin-syntax-dynamic-import",
        ["@babel/plugin-proposal-class-properties", {
            "loose": true
        }]
    ]
}